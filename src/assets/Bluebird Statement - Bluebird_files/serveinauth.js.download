
var tid = document.getElementById('inauthtid');
var url = document.getElementById('inauthcollectorurl');
var sid = document.getElementById('inauthsiteid');
var cflags = document.getElementById('inauthcflags');

window._cc = window._cc || []; _cc.push(['ci', {
    'sid': sid.value,
    'tid': tid.value
}]);

if(cflags!=undefined && cflags.value!=null){
    _cc.push(['cf', cflags.value]);
}
_cc.push(['run', ('https:' == document.location.protocol ? 'https://' : 'http://') + url.value]);
(function () {
    var c = document.createElement('script');
    c.type = 'text/javascript';
    c.async = true;
    c.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + url.value + '/cc.js?sid=c3afbc3665a61ff1&ts='+tid.value;
    var s = document.getElementsByTagName('script')[0];
    s.parentNode.insertBefore(c, s); })();

